datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum Privileges {
  MANAGER
  EMPLOYEE
}

model user {
  id        Int    @id @default(autoincrement())
  name      String @unique
  password  String
  education String
  privileges Privileges
  email      String
}


